{
  "web-nextjs-standard": {
    "description": "Next.js App Router Enterprise Structure",
    "structure": [
      "app/(auth)",
      "app/(dashboard)",
      "app/api",
      "components/ui",
      "components/shared",
      "lib/utils",
      "hooks",
      "services",
      "types"
    ]
  },
  "web-vite-vanilla": {
    "description": "Lightweight Vite + Vanilla JS SPA",
    "structure": [
      "src/app",
      "src/pages",
      "src/components",
      "src/services",
      "src/utils",
      "src/config",
      "src/assets",
      "src/styles",
      "public"
    ]
  },
  "backend-nodejs-express": {
    "description": "Restful API with Clean Architecture",
    "structure": [
      "src/controllers",
      "src/services",
      "src/models",
      "src/routes",
      "src/middlewares",
      "src/config",
      "src/utils"
    ]
  },
  "backend-python-fastapi": {
    "description": "Python FastAPI with Domain-Driven Structure",
    "structure": [
      "app/api/routes",
      "app/api/deps",
      "app/core",
      "app/models",
      "app/schemas",
      "app/services",
      "app/db",
      "tests"
    ]
  },
  "mobile-react-native": {
    "description": "Cross-platform Mobile App Structure",
    "structure": [
      "src/screens",
      "src/components",
      "src/navigation",
      "src/store",
      "src/services",
      "src/theme",
      "src/assets"
    ]
  }
}